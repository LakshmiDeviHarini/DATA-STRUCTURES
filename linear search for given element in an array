#finding given element in an array
public class Search
 {
    public static void main(String args[])
    {
        int a[]={76,23,89,2,78};
        int key=56;
        for( int i=0;i<=4;i++)
        {
            if (a[i]==key)
            {
                System.out.println("element found");
                System.exit(0);
                
            }
            
        }
    }
    
}
